AndOperate
{
    Lite : SameOperate
           {
               Lite : ValueOperate
                      {
                          Value : IntValue
                                  {
                                      Value : 1,
                                  },
                      },
               Rite : ValueOperate
                      {
                          Value : IntValue
                                  {
                                      Value : 2,
                                  },
                      },
           },
    Rite : ValueOperate
           {
               Value : BoolValue
                       {
                           Value : true,
                       },
           },
},
