{
  "name": "class-lang-vscode-ext",
  "displayName": "Class",
  "description": "Class programming language",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/alanng1/class-lang"
  },
  "publisher": "AlanNg",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "class",
      "aliases": ["Class", "class"],
      "extensions": [".cla"],
      "configuration": "./language-configuration.json"
    },{
      "id": "classport",
      "filenames": ["Class.Port"]
    }],
    "grammars": [{
      "language": "class",
      "scopeName": "source.class",
      "path": "./syntaxes/class.tmLanguage.json"
    },{
      "language": "classport",
      "scopeName": "source.classport",
      "path": "./syntaxes/classport.tmLanguage.json"
    }]
  }
}
